# ZkToken Contracts — Environment Variables
# Copy to .env and fill in values before deploying.

# Deployer wallet private key (no 0x prefix)
DEPLOYER_PRIVATE_KEY=

# ERC20 token to wrap. Leave empty to auto-deploy TestToken on Fuji.
TOKEN_ADDRESS=

# Deployed Poseidon(2) contract address. Leave empty to deploy via script.
POSEIDON_ADDRESS=

# Snowtrace API key for contract verification
SNOWTRACE_API_KEY=

# ── Filled post-deployment ──
SHIELDED_POOL_ADDRESS=
TRANSFER_VERIFIER_ADDRESS=
WITHDRAW_VERIFIER_ADDRESS=
